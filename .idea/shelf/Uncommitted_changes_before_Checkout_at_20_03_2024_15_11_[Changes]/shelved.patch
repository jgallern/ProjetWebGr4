+   FOREIGN KEY(ID_Centre) REFERENCES Centre(ID_Centre),
+);
+
+CREATE TABLE Notes(
+   ID_Note INT NOT NULL AUTO_INCREMENT ,
+   Note INT,
+   Commentaire VARCHAR(280),
+   PRIMARY KEY(ID_Note)
+);
+
