+   FOREIGN KEY(ID_Personne) REFERENCES Personne(ID_Personne)
+);
+
+CREATE TABLE Adresse(
+   ID_Adresse INT NOT NULL AUTO_INCREMENT,
+   Numero_rue INT,
+   Nom_rue VARCHAR(200),
+   Ville VARCHAR(100),
+   ID_Centre INT NOT NULL,
+   PRIMARY KEY(ID_Adresse),
+);
